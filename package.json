{
  "name": "convenable",
  "version": "0.0.966",
  "description": "a bunch of disparate, but convenient functions",
  "license": "MIT",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "homepage": "https://github.com/Xananax/convenient/",
