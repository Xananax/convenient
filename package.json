    "jsdom": "^13.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.2",
    "rimraf": "^2.6.3",
    "typescript": "^3.2.2"
  },
  "eslintConfig": {
    "extends": "react-app"
