{
  "name": "convenable",
  "version": "0.0.92",
  "description": "a bunch of disparate, but convenient functions",
  "license": "MIT",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "homepage": "https://github.com/Xananax/convenient/",
  "author": "xananax@yelostudio.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xananax/convenient/"
  },
  "bugs": {
    "url": "https://github.com/Xananax/convenient/issues"
  },
  "devDependencies": {
    "gh-pages": "latest",
    "react-dom": "latest",
    "react": "latest",
    "react-scripts-ts": "latest",
    "rimraf": "latest",
    "typescript": "latest",
    "@types/react-dom": "latest",
    "@types/react": "latest",
    "@types/jest": "latest",
    "@types/node": "latest"
  },
  "scripts": {
    "demo:start": "react-scripts-ts start",
    "demo:build": "react-scripts-ts build",
    "demo:test": "react-scripts-ts test --env=jsdom",
    "demo": "npm run demo:build",
    "package:build": "tsc --moduleResolution Node --module commonjs --target es5 --outDir dist --declaration",
    "package:clean": "rimraf ./dist",
    "start": "npm run demo:start",
    "package": "npm run package:clean && npm run package:build",
    "predeploy": "npm run package && npm run demo",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "utils",
    "typescript"
  ]
}
